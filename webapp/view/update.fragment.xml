	xmlns:f="sap.ui.layout.form"
   xmlns:core="sap.ui.core">
<Dialog id="UpdateDialog" class="sapUiSizeCozy" resizable="true" draggable="true" title="{i18n>TitUpdate}" contentHeight="400px"
		contentWidth="230px">
		<ScrollContainer height="60%" width="40%"
			horizontal="true" vertical="false">
			
		</ScrollContainer>
		<beginButton>
