                                <micro:data>
                                    <micro:ComparisonMicroChartData title="Price" value="{CompModel>/d/Price}" color="Good"/>
                                    <micro:ComparisonMicroChartData title="GrossAmount" value="{CompModel>/d/GrossAmount}" color="Good"/>
                                    <!-- <micro:ComparisonMicroChartData title="APAC" value="-20" color="Error"/> -->
                                </micro:data>
                            </micro:ComparisonMicroChart>

                            <!-- scale - M,B, K  Amount, state- positive,negative , Max , Min value  critiacl - M -->
                        </FlexBox>

                        <FlexBox class="sapUiMediumMarginBegin sapUiTinyMarginTop tileLayout" height="70px" width="170px" renderType="Bare">
                            <micro:LineMicroChart id="IDLineChart" threshold="5" size="Responsive" press="press" leftTopLabel="0" leftBottomLabel="Q1 Start" rightTopLabel="10" rightBottomLabel="Q1 End">
                                <micro:lines>
                                    <micro:LineMicroChartLine>
                                        <micro:points>
                                            <micro:LineMicroChartEmphasizedPoint x="9" y="4" color="Error" show="true"/>
                                            <micro:LineMicroChartEmphasizedPoint x="2" y="5"/>
                                            <!-- <micro:LineMicroChartEmphasizedPoint x="{oLineModel>/d/results/2/ItemCount}" y="Ariba"/>
                                            <micro:LineMicroChartEmphasizedPoint x="{oLineModel>/d/results/3/ItemCount}" y="FG"/>
                                            <micro:LineMicroChartEmphasizedPoint x="{oLineModel>/d/results/4/ItemCount}" y="ECC"/>
                                            <micro:LineMicroChartEmphasizedPoint x="{oLineModel>/d/results/5/ItemCount}" y="JAVA" color="Good" show="true"/> -->
											 <micro:LineMicroChartEmphasizedPoint x="3" y="6" color="Good" show="true"/>
											  <!-- <micro:LineMicroChartEmphasizedPoint x="{oLineModel>/d/results/7/ItemCount}" y="Azure" color="Good" show="true"/>
                                       
									   -->

									   <!-- x = no of system errors, y = period, Week1 , Week2 ; threshold =5 ,  w1= 4, w2=8, -->
									    </micro:points> 
                                    </micro:LineMicroChartLine>

                                    <!-- use  for short intervals - upto quarter , define min and max range critical - x, y plotting  control max boundaries for x and y axis - maxXValue-->

                                    <!-- plotting x,y axis in odata -   use reference https://sapui5.hana.ondemand.com/sdk/test-resources/sap/viz/Charting.html
                                                                                                                                                  if we 6 values  max = 1000 , min = -500 , x= -300    y=1 ,  x=200 y=2 ,-> x represents amount, y- represent position.
                                                                                                                                                
                                                                                                                                                 ?? https://ui5.sap.com/1.96.31/#/entity/sap.suite.ui.microchart.LineMicroChart/sample/sap.suite.ui.microchart.sample.LineMicroChartInGridTable/code/SampleData.json -->
                                </micro:lines>
