<mvc:View xmlns:core="sap.ui.core" controllerName="ux.zanalytics.controller.View1" xmlns:f="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns:micro="sap.suite.ui.microchart" xmlns="sap.m">
    <Page id="page" showHeader="false">
        <content>

            <VBox alignContent="Stretch" fitContainer="true" height="100%" width="100%"
                class="sapUiMediumMargin">

                <!-- <Text text="page"></Text> -->
                <HBox >


                    <!-- <Text text="section 1"></Text> -->

                    <Image class="sapUiTinyMarginEnd" src="../image/tata logo transparent.png" width="70px" height="70px"></Image>
                    <!-- <core:Icon src="sap-icon://sap-ui5"></core:Icon> -->

                    <HBox>


                        <FlexBox class="sapUiMediumMarginBegin sapUiTinyMarginTop tileLayout" height="100px" width="170px" renderType="Bare">
                            <micro:ComparisonMicroChart scale="M" maxValue="" minValue="" press="press">
                                <micro:data>
                                    <micro:ComparisonMicroChartData title="Americas" value="10" color="Good"/>
                                    <micro:ComparisonMicroChartData title="EMEA" value="50" color="Good"/>
                                    <micro:ComparisonMicroChartData title="APAC" value="-20" color="Error"/>
                                </micro:data>
                            </micro:ComparisonMicroChart>

                            <!-- scale - M,B, K  Amount, state- positive,negative , Max , Min value  critiacl - M -->
                        </FlexBox>

                        <FlexBox class="sapUiMediumMarginBegin sapUiTinyMarginTop tileLayout" height="70px" width="170px" renderType="Bare">
                            <micro:LineMicroChart threshold="40" size="Responsive" press="press" leftTopLabel="0M" leftBottomLabel="Q1 Start" rightTopLabel="80M" rightBottomLabel="Q1 End">
                                <micro:lines>
                                    <micro:LineMicroChartLine>
                                        <micro:points>
                                            <micro:LineMicroChartEmphasizedPoint x="0" y="35" color="Error" show="true"/>
                                            <micro:LineMicroChartEmphasizedPoint x="8" y="53"/>
                                            <micro:LineMicroChartEmphasizedPoint x="20" y="10"/>
                                            <micro:LineMicroChartEmphasizedPoint x="30" y="30"/>
                                            <micro:LineMicroChartEmphasizedPoint x="40" y="52"/>
                                            <micro:LineMicroChartEmphasizedPoint x="100" y="73" color="Good" show="true"/>
                                        </micro:points>
                                    </micro:LineMicroChartLine>

                                    <!-- use  for short intervals - upto quarter , define min and max range critical - x, y plotting  control max boundaries for x and y axis - maxXValue-->

                                    <!-- plotting x,y axis in odata -   use reference https://sapui5.hana.ondemand.com/sdk/test-resources/sap/viz/Charting.html
                                                                                                                                                  if we 6 values  max = 1000 , min = -500 , x= -300    y=1 ,  x=200 y=2 ,-> x represents amount, y- represent position.
                                                                                                                                                
                                                                                                                                                 ?? https://ui5.sap.com/1.96.31/#/entity/sap.suite.ui.microchart.LineMicroChart/sample/sap.suite.ui.microchart.sample.LineMicroChartInGridTable/code/SampleData.json -->
                                </micro:lines>
                            </micro:LineMicroChart>
                        </FlexBox>

                        <Text class=" sapUiMediumMarginBegin sapUiTinyMarginTop tileLayout Tile1" text="Critical System - CRM "></Text>
